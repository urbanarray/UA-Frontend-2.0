{
  "name": "frontend-2.0",
  "version": "1.0.0",
  "description": "## Getting Started",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node --exec babel-node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urbanarray/UA-Frontend-2.0.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/urbanarray/UA-Frontend-2.0/issues"
  },
  "homepage": "https://github.com/urbanarray/UA-Frontend-2.0#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "pg": "^7.11.0",
    "sequelize": "^5.8.10",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "concurrently": "^4.1.0",
    "eslint": "^5.12.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-react": "^7.13.0",
    "if-env": "^1.0.4",
    "nodemon": "^1.19.1"
  },
  "pre-commit": "lint:staged"
}
